{
  "name": "react-js-pagination-update",
  "version": "3.0.3",
  "description": "Simple, easy to use component for pagination. Compatible with bootstrap paginator stylesheets",
  "main": "./dist/Pagination.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-example": "rimraf src/example/dist",
    "build": "npm run clean && cross-env NODE_ENV=production node build.js",
    "build-example": "npm run clean-example && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "start": "node devServer.js",
    "lint": "eslint 'src/components/**/*.js'",
    "test": "mocha --compilers js:babel-register --require ./test-setup.js 'src/**/*-test.js'",
    "validate": "npm-run-all lint test",
    "test:watch": "mocha -w --compilers js:babel-register --require ./test-setup.js 'src/**/*-test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bymagnum/react-js-pagination-update"
  },
  "keywords": [
    "react",
    "reactjs",
    "pagination",
    "paginator",
    "bootstrap",
    "reload",
    "webpack",
    "babel",
    "react-transform",
    "react-component"
  ],
  "author": "Sergey Vayser <wice24@gmail.com> (http://github.com/vayser)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/bymagnum/react-js-pagination-update/issues"
  },
  "homepage": "https://github.com/bymagnum/react-js-pagination-update",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "7.1.1",
    "babel-loader": "9.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^5.3.3",
    "chai": "^5.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "9.17.0",
    "eslint-plugin-react": "7.37.3",
    "expect": "^29.7.0",
    "express": "^4.21.2",
    "file-loader": "^6.2.0",
    "jsdom": "^25.0.1",
    "less": "^4.2.1",
    "less-loader": "^12.2.0",
    "mkdirp": "^3.0.1",
    "mocha": "^11.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "react-addons-test-utils": "15.x.x - 16.x.x",
    "react-dom": "19.x.x",
    "react-hot-loader": "^4.13.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-test-renderer": "^19.0.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "uglify-js": "^3.19.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.97.1",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "paginator": "^1.0.0",
    "prop-types": "15.x.x - 16.x.x",
    "react": "19.x.x"
  }
}
